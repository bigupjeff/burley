{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"customTemplates": [
		{ "name": "template-blank", "title": "Blank Page - Only Content" },
		{ "name": "services", "title": "Services Archive" },
		{ "name": "full-width", "title": "Full Width" },
		{ "name": "sidebar-right", "title": "Sidebar on the Right" }
	],
	"templateParts": [
		{ "name": "header", "title": "Header", "area": "header" },
		{ "name": "header-centered", "title": "Header Centered", "area": "header" },
		{ "name": "header-sticky-sliding", "title": "Header Sticky Sliding", "area": "header" },
		{ "name": "footer", "title": "Footer", "area": "footer" },
		{ "name": "footer-4-columns", "title": "Footer 4 Columns", "area": "footer" },
		{ "name": "sidebar", "title": "Sidebar", "area": "uncategorized" }
	],
	"settings": {
		"appearanceTools": true,
		"color": {
			"defaultDuotone": false,
			"defaultPalette": false,
			"defaultGradients": false,
			"duotone": [
				{
					"colors": ["#111111", "#ffffff"],
					"slug": "duotone-1",
					"name": "Black and white"
				},
				{
					"colors": ["#111111", "#C2A990"],
					"slug": "duotone-2",
					"name": "Black and sandstone"
				},
				{
					"colors": ["#111111", "#D8613C"],
					"slug": "duotone-3",
					"name": "Black and rust"
				},
				{
					"colors": ["#111111", "#B1C5A4"],
					"slug": "duotone-4",
					"name": "Black and sage"
				},
				{
					"colors": ["#111111", "#B5BDBC"],
					"slug": "duotone-5",
					"name": "Black and pastel blue"
				}
			],
			"gradients": [
                {
                    "slug": "mysterion",
                    "gradient": "radial-gradient(circle at 100% 100%,#0c4 20%,#497fb1 80%)",
                    "name": "Mysterion (radial)"
                }
			],
			"palette": [
				{ "slug": "bur-bg", "color": "#072c50", "name": "Theme Background" },
				{ "slug": "bur-fg", "color": "#ffffff", "name": "Theme Foreground" },
				{ "slug": "bur-bg-alt", "color": "#fff2", "name": "Theme Background Alt" },
				{ "slug": "bur-fg-alt", "color": "#fcfcfc", "name": "Theme Foreground Alt" },
				{ "slug": "bur-accent", "color": "#f89549", "name": "Theme Accent" },
				{ "slug": "pink-lightest", "color": "hsl(303, 53%, 88%)", "name": "Pink Lightest" },
				{ "slug": "pink-light", "color": "hsl(303, 53%, 60%)", "name": "Pink Light" },
				{ "slug": "pink", "color": "hsl(303, 53%, 50%)", "name": "Pink" },
				{ "slug": "pink-dark", "color": "hsl(303, 53%, 40%)", "name": "Pink Dark" },
				{ "slug": "pink-darkest", "color": "hsl(302, 53%, 20%)", "name": "Pink Darkest" },
				{ "slug": "purple-lightest", "color": "hsl(263, 53%, 88%)", "name": "Purple Lightest" },
				{ "slug": "purple-light", "color": "hsl(264, 53%, 60%)", "name": "Purple Light" },
				{ "slug": "purple", "color": "hsl(264, 53%, 50%)", "name": "Purple" },
				{ "slug": "purple-dark", "color": "hsl(264, 53%, 40%)", "name": "Purple Dark" },
				{ "slug": "purple-darkest", "color": "hsl(263, 53%, 20%)", "name": "Purple Darkest" },
				{ "slug": "blue-lightest", "color": "hsl(207, 53%, 88%)", "name": "Blue Lightest" },
				{ "slug": "blue-light", "color": "hsl(207, 53%, 60%)", "name": "Blue Light" },
				{ "slug": "blue", "color": "hsl(207, 53%, 50%)", "name": "Blue" },
				{ "slug": "blue-dark", "color": "hsl(207, 53%, 40%)", "name": "Blue Dark" },
				{ "slug": "blue-darkest", "color": "hsl(207, 53%, 20%)", "name": "Blue Darkest" },
				{ "slug": "green-lightest", "color": "hsl(136, 53%, 88%)", "name": "Green Lightest" },
				{ "slug": "green-light", "color": "hsl(136, 53%, 60%)", "name": "Green Light" },
				{ "slug": "green", "color": "hsl(136, 53%, 50%)", "name": "Green" },
				{ "slug": "green-dark", "color": "hsl(136, 53%, 40%)", "name": "Green Dark" },
				{ "slug": "green-darkest", "color": "hsl(136, 53%, 20%)", "name": "Green Darkest" },
				{ "slug": "yellow-lightest", "color": "hsl(62, 76%, 88%)", "name": "Yellow Lightest" },
				{ "slug": "yellow-light", "color": "hsl(61, 75%, 60%)", "name": "Yellow Light" },
				{ "slug": "yellow", "color": "hsl(61, 76%, 50%)", "name": "Yellow" },
				{ "slug": "yellow-dark", "color": "hsl(61, 75%, 40%)", "name": "Yellow Dark" },
				{ "slug": "yellow-darkest", "color": "hsl(62, 76%, 20%)", "name": "Yellow Darkest" },
				{ "slug": "orange-lightest", "color": "hsl(30, 76%, 88%)", "name": "Orange Lightest" },
				{ "slug": "orange-light", "color": "hsl(30, 75%, 60%)", "name": "Orange Light" },
				{ "slug": "orange", "color": "hsl(30, 76%, 50%)", "name": "Orange" },
				{ "slug": "orange-dark", "color": "hsl(30, 75%, 40%)", "name": "Orange Dark" },
				{ "slug": "orange-darkest", "color": "hsl(30, 76%, 20%)", "name": "Orange Darkest" },
				{ "slug": "red-lightest", "color": "hsl(0, 76%, 88%)", "name": "Red Lightest" },
				{ "slug": "red-light", "color": "hsl(0, 75%, 60%)", "name": "Red Light" },
				{ "slug": "red", "color": "hsl(0, 76%, 50%)", "name": "Red" },
				{ "slug": "red-dark", "color": "hsl(0, 75%, 40%)", "name": "Red Dark" },
				{ "slug": "red-darkest", "color": "hsl(0, 76%, 20%)", "name": "Red Darkest" },
				{ "slug": "white", "color": "hsl(0, 0%, 100%)", "name": "White" },
				{ "slug": "grey-lightest", "color": "hsl(0, 0%, 93%)", "name": "Grey Lightest" },
				{ "slug": "grey-light", "color": "hsl(0, 0%, 73%)", "name": "Grey Light" },
				{ "slug": "grey", "color": "hsl(0, 0%, 50%)", "name": "Grey" },
				{ "slug": "grey-dark", "color": "hsl(0, 0%, 30%)", "name": "Grey Dark" },
				{ "slug": "grey-darkest", "color": "hsl(0, 0%, 20%)", "name": "Grey Darkest" },
				{ "slug": "black", "color": "hsl(0, 0%, 0%)", "name": "Black" }
			],
			"link": true
		},
		"typography": {
			"lineHeight": true,
			"customFontSize": true,
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "Open Sans",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "300 800",
							"src": [
								"file:./assets/fonts/Open_Sans/OpenSans-VariableFont_wdth,wght.ttf"
							]
						},
						{
							"fontFamily": "Open Sans",
							"fontStretch": "normal",
							"fontStyle": "italic",
							"fontWeight": "300 800",
							"src": [
								"file:./assets/fonts/Open_Sans/OpenSans-Italic-VariableFont_wdth,wght.ttf"
							]
						}
					],
					"name": "Open Sans",
					"slug": "body"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Inter",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "100 900",
							"src": [
								"file:./assets/fonts/Inter/Inter-VariableFont_slnt,wght.ttf"
							]
						}
					],
					"fontFamily": "Inter, Arial, sans-serif",
					"name": "Inter",
					"slug": "heading"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Wellfleet",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/Wellfleet/Wellfleet-Regular.ttf"
							]
						}
					],
					"fontFamily": "Wellfleet, 'Rockwell Extra Bold', Palatino, serif",
					"name": "Wellfleet",
					"slug": "wellfleet"
				}
			],
			"fontSizes": [
				{ "name": "Small", "size": "0.8125rem", "slug": "small" },
				{ "name": "Medium", "size": "1rem", "slug": "medium" },
				{ "name": "Large", "size": "1.25rem", "slug": "large" },
				{ "name": "Extra Large", "size": "1.625rem", "slug": "x-large" },
				{ "name": "Extra Extra Large", "size": "2.0625rem", "slug": "xx-large" },
				{ "name": "Extra Extra Extra Large", "size": "2.625rem", "slug": "xxx-large" },
				{ "name": "Display Small", "size": "3.3125rem", "slug": "display-small" },
				{ "name": "Display Medium", "size": "4.25rem", "slug": "display-medium" },
				{ "name": "Display Large", "size": "5.375rem", "slug": "display-large" },
				{ "name": "Display Extra Large", "size": "6.875rem", "slug": "display-x-large" }
			]
		},
		"layout": {
			"contentSize": "35rem",
			"wideSize": "70rem"
		},
		"useRootPaddingAwareAlignments": true,
		"spacing": {
			"customSpacingSize": true,
			"padding": true,
			"margin": true,
			"blockGap": true,
			"units": [ "rem", "vh", "vw", "%" ],
			"spacingSizes": [
				{ "name": "1 (0.5rem)", "slug": "20", "size": "0.5rem" },
				{ "name": "2 (0.75rem)", "slug": "30", "size": "0.75rem" },
				{ "name": "3 (1rem)", "slug": "40", "size": "1rem" },
				{ "name": "4 (2rem)", "slug": "50", "size": "2rem" },
				{ "name": "5 (3.5rem)", "slug": "60", "size": "3.5rem" },
				{ "name": "6 (6rem)", "slug": "70", "size": "6rem" },
				{ "name": "7 (10rem)", "slug": "80", "size": "10rem" }
			]
		},
		"border": {
			"color": true,
			"radius": true,
			"style": true,
			"width": true
		},
		"shadow": {
			"presets": [
				{ "name": "Small", "slug": "theme", "shadow": "0 0.25rem 0.5em 0.25rem #0006,0 0.125rem 0.25rem 0.125rem #0006" },
				{ "name": "Small Soft", "slug": "theme-soft", "shadow": "0 0 1.5rem -0.45rem #0008,0 0.125rem 0.25rem 0.1rem #0003" },
				{ "name": "Inset", "slug": "theme-inset", "shadow": "inset -0.1rem 0.2rem 0.5rem 0.2rem #000c" }
			]
		},
		"dimensions": { "minHeight": true },
		"position": { "sticky": true },
		"custom": {
			"body-class": "bur-theme-default",
			"fluid-root": "var(--bur--fluid-root)",
			"gutter": {
				"small": "var( --wp--preset--spacing--40 )",
				"medium": "clamp( var( --wp--preset--spacing--50 ), 4vw, var( --wp--preset--spacing--60 ) )",
				"large": "var( --wp--preset--spacing--70 )",
				"x-large": "var( --wp--preset--spacing--80 )"
			},
			"border-size": "0.1875rem",
			"border-radius": {
				"small": "0.5rem",
				"medium": "1rem",
				"large": "2rem"
			},
			"padding-page": "clamp( var( --wp--preset--spacing--40 ), 6vw, var( --wp--preset--spacing--70 ) )",
			"transition": { "hover-time": "0.35s" }
		},
		"blocks": {
			"core/group": {
				"spacing": {
					"margin": true,
					"padding": true,
					"blockGap": true
				}
			}
		}
	},
	"styles": {
		"color": {
			"background": "var(--wp--preset--color--bur-bg)",
			"text": "var(--wp--preset--color--bur-fg)"
		},
		"typography": {
			"fontSize": "var(--wp--preset--font-size--medium)",
			"fontFamily": "var(--wp--preset--font-family--body)",
			"fontWeight": "400",
			"lineHeight": "1.6"
		},
		"spacing": {
			"blockGap": "var(--wp--preset--spacing--50)",
			"padding": {
				"top": "0",
				"bottom": "0",
				"right": "var(--wp--custom--padding-page)",
				"left": "var(--wp--custom--padding-page)"
			}
		},
		"elements": {
			"button": {
				"border": { "width": "0", "style": "solid" },
				"typography": { "textTransform": "uppercase", "fontWeight": "600" },
				":hover": { "border": { "radius": "var(--wp--custom--border-radius--small)" } }
			},
			"link": { "color": { "text": "inherit" } },
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--heading)",
					"fontWeight": "900",
					"lineHeight": "1.3"
				}
			},
			"h1": { "typography": { "fontSize": "var(--wp--preset--font-size--xxx-large)" } },
			"h2": { "typography": { "fontSize": "var(--wp--preset--font-size--xx-large)" } },
			"h3": { "typography": { "fontSize": "var(--wp--preset--font-size--x-large)" } },
			"h4": { "typography": { "fontSize": "var(--wp--preset--font-size--large)" } },
			"h5": { "typography": { "fontSize": "var(--wp--preset--font-size--medium)" } },
			"h6": { "typography": { "fontSize": "var(--wp--preset--font-size--small)" } }
		},
		"blocks": {
			"core/buttons": { "spacing": { "blockGap": "var(--wp--custom--gutter--small)" } },
			"core/button": {
				"css": "transition: border-radius var(--wp--custom--transition--hover-time) ease-in-out; transition-delay: 0ms !important;",
				"spacing": {
					"padding": {
						"top": "var(--wp--preset--spacing--40)",
						"bottom": "var(--wp--preset--spacing--40)",
						"left": "var(--wp--preset--spacing--50)",
						"right": "var(--wp--preset--spacing--50)"
					}
				},
				"color": { "background": "var(--wp--preset--color--bur-accent)", "text": "var(--wp--preset--color--bur-fg-alt)" },
				"variations": {
					"outline": {
						"color": { "background": "#0000", "text": "currentColor" },
						"border": { "width": "var(--wp--custom--border-size)", "color": "currentColor" },
						"spacing": {
							"padding": {
								"top": "calc( var(--wp--preset--spacing--40) - var(--wp--custom--border-size) )",
								"bottom": "calc( var(--wp--preset--spacing--40) - var(--wp--custom--border-size) )",
								"left": "calc( var(--wp--preset--spacing--50) - var(--wp--custom--border-size) )",
								"right": "calc( var(--wp--preset--spacing--50) - var(--wp--custom--border-size) )"
							}
						}
					}
				}
			},
			"core/comment-author-name": { "typography": { "fontSize": "var(--wp--preset--font-size--medium)" } },
			"core/comment-content": {
				"border": { "radius": "var(--wp--custom--border-radius--small)" },
				"color": { "background": "var(--wp--preset--color--bur-bg-alt)" },
				"spacing": {
					"padding": {
						"bottom": "var(--wp--preset--spacing--50)",
						"left": "var(--wp--preset--spacing--40)",
						"right": "var(--wp--preset--spacing--40)",
						"top": "var(--wp--preset--spacing--50)"
					}
				}
			},
			"core/comment-date": {
				"typography": {
					"textTransform": "uppercase",
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/comment-edit-link": {
				"typography": {
					"textTransform": "uppercase",
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/comment-reply-link": {
				"typography": { "textTransform": "uppercase" },
				"elements": {
					"link": {
						"typography": { "fontSize": "var(--wp--preset--font-size--small)" },
						"color": { "text": "var(--wp--preset--color--bur-accent)" }
					}
				}
			},
			"core/group": {
				"css": "row-gap:var(--wp--custom--gutter--large);",
				"spacing": { "blockGap": "var(--wp--custom--gutter--medium)" }
			},
			"core/columns": {
				"css": "row-gap:var(--wp--custom--gutter--large);",
				"spacing": { "blockGap": "var(--wp--custom--gutter--medium)" }
			},
			"core/post-template": {
				"css": "row-gap:var(--wp--custom--gutter--large);",
				"spacing": { "blockGap": "var(--wp--custom--gutter--medium)" }
			},
			"core/navigation": { "spacing": { "blockGap": "var(--wp--custom--gutter--small)" } },
			"core/quote": { "typography": { "fontSize": "var(--wp--preset--font-size--x-large)" } },
			"core/search": { "border": { "radius": "0.375rem" } },
			"core/code": {
				"typography": { "fontFamily": "monospace" },
				"color": { "text": "#8ebff5", "background": "#222" },
				"spacing": { "padding": { "bottom": "var(--wp--preset--spacing--50)", "left": "var(--wp--preset--spacing--30)", "right": "var(--wp--preset--spacing--30)", "top": "var(--wp--preset--spacing--50)" } },
				"shadow": "var(--wp--preset--shadow--theme-inset)"
			},

			"core/table": {
				"css": "border-color:var(--wp--preset--color--bur-fg); border-bottom: solid 0.25rem; & th{ color:var(--wp--preset--color--bur-bg); background:var(--wp--preset--color--bur-fg); border: 0px; } & td{ border: 0px; } & tr:nth-child(even){ background: #fff1; }",
				"color": { "text": "var(--wp--preset--color--bur-fg)" }
			}
		}
	}
}